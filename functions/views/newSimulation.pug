extends layout

block content
    h1 Create New Simulation
    
    form(method='POST' action='' onsubmit="return validateForm()")
        div.form-group(id='formType')
            div(id='duplicatedInputSet')

                label(id="editNewSimulationLabel") Edit/New Simulation Set:
                select#editNewSimulationSelect.form-control(type='select', placeholder='', name='editNewSimulationSelect', required='true', onchange='editNewSimulationChange(this.id, this.value)')
                    for simulation in simulations
                        option(value=simulation.activities) #{simulation.name}

                br()

                label(id="activityLabel0") Activity Type:
                select#activity0.form-control(type='select', placeholder='', name='activity_type0', required='true', onchange='activityTypeChange(this.id)')
                    for activity in activityTypes
                        option(value=activity._id) #{activity.name}
                label(id="foodLabel0") Food Type:
                select#food0.form-control(type='select', placeholder='', name='food_type0', required='false', onchange='foodTypeChange(this.id)')
                    for food in foodTypes
                        option(value=food._id) #{food.name}
                label(id="exerciseLabel0") Exercise Type:
                select#exercise0.form-control(type='select', placeholder='', name='exercise_type0', required='false', onchange='exerciseTypeChange(this.id)')
                    for exercise in exerciseTypes
                        option(value=exercise._id) #{exercise.name}

                label(id='newFoodTitle0') Create New Food Type:
                label(for='newFoodName', id="newFoodNameLabel0") Name:
                input(type='text', name='newFoodName0', id="newFoodNameInput0")
                label(for='newFoodServingSize', id="newFoodServingSizeLabel0") Serving Size: 
                input(type='number', min='0', name='newFoodServingSize0', id="newFoodServingSizeInput0", onkeypress="return isNumberKey(event)")
                label(for='newFoodFat', id="newFoodFatLabel0") Fat: 
                input(type='number', min='0', name='newFoodFat0', id="newFoodFatInput0", onkeypress="return isNumberKey(event)")
                label(for='newFoodProtein', id="newFoodProteinLabel0") Protein: 
                input(type='number', min='0', name='newFoodProtein0', id="newFoodProteinInput0", onkeypress="return isNumberKey(event)")
                label(for='newFoodRAG', id="newFoodRAGLabel0") Readily Available Glucose (RAG): 
                input(type='number', min='0', name='newFoodRAG0', id="newFoodRAGInput0", onkeypress="return isNumberKey(event)")
                label(for='newFoodSAG', id="newFoodSAGLabel0") Slowly Available Glucose (SAG): 
                input(type='number', min='0', name='newFoodSAG0', id="newFoodSAGInput0",onkeypress="return isNumberKey(event)")

                label(id='newExerciseTitle0') Create New Exercise Type:
                label(for='newExerciseName', id="newExerciseNameLabel0") Name: 
                input(type='text', name='newExerciseName0', id="newExerciseNameInput0")
                label(for='newExerciseIntensity', id="newExerciseIntensityLabel0") Intensity: 
                input(type='number', min='0', name='newExerciseIntensity0', id="newExerciseIntensityInput0", onkeypress="return isNumberKey(event)")

                label(id='dayNumberLabel0') Date (Day:Hour:Minute) : 
                div.form-inline(id='dayInLine0')
                    input(type='number', min='1', name='dayNumberInput0', id='dayNumberInput0', onkeypress="return isNumberKey(event)", oninput='dayChange(this.value, this)')
                    label(id='firstSplit') :
                    select#hour0.form-control(type='select', placeholder='', name='hourSelect0', required='false', style='width:200px', onchange='hourChange(this.value, this)')
                        for hour in hours
                            option(value=hour._id) #{hour.name}
                    label(id='firstSplit1') :
                    select#minute0.form-control(type='select', placeholder='', name='minuteSelect0', required='false', style='width:200px', onchange='minuteChange(this.value, this)')
                        for minute in minutes
                            option(value=minute._id) #{minute.name}

                button.btn.btn-primary(type='button', id='deleteEntry0', onclick='deleteEntry(this.id)') Delete Entry

                label(for='qtyFood', id="foodQtyLabel0") Servings: 
                input(type='number', min='0', name='foodQtyInput0', id='foodQtyInputID0', onkeypress="return isNumberKey(event)")

                label(for='qtyEx', id='exerciseQtyLabel0') Count: 
                input(type='number', min='0', name='exerciseQtyInput0', id='exerciseQtyInputID0', onkeypress="return isNumberKey(event)")
                input(type='text', name='deleted0', id='deletedID0')

        button.btn.btn-primary(type='button', id='newDuplicateSetBtn', onclick='duplicate(false, null, null, null, null, null, null)') +
        br()
        br()
        br()

        label(id='endTimeLabel') Simulation End Time (Day:Hour:Minute) :
        div.form-inline(id='simEndTime')
            input(type='number', min='1', name='endTimeDayInput', id='endTimeDayInput', onkeypress="return isNumberKey(event)")
            label(id='break1') :
            select#endTimeHourSelect.form-control(type='select', placeholder='', name='endTimeHourSelect', required='false', style='width:200px')
                for hour in hours
                    option(value=hour._id) #{hour.name}
            label(id='break2') :
            select#endTimeMinuteSelect.form-control(type='select', placeholder='', name='endTimeMinuteSelect', required='false', style='width:200px')
                for minute in minutes
                    option(value=minute._id) #{minute.name}

        br()
        br()
        button.btn.btn-primary(type='submit', id='submitButton') Submit

