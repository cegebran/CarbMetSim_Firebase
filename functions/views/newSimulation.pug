                    document.getElementById("newExerciseNameInput" + index).style.display = "none";
                    document.getElementById("newExerciseIntensityLabel" + index).style.display = "none";
                    document.getElementById("newExerciseIntensityInput" + index).style.display = "none";
                    document.getElementById("foodQtyLabel" + index).style.display = "none";
                    document.getElementById("foodQtyInputID" + index).style.display = "none";
                    document.getElementById("exerciseQtyLabel" + index).style.display = "none";
                    document.getElementById("exerciseQtyInputID" + index).style.display = "none";
                    document.getElementById("deleteEntry" + index).style.display = "block";
                    document.getElementById("dayInLine" + index).style.display = "none";
                    document.getElementById("dayNumberLabel" + index).style.display = "none";

                    changeAllOfIndexToInheritBorder(index);
                }
                if(activities.value == "Food")
                {
                    food_exercise[index - 1] = "Food";
                    document.getElementById("food" + index).style.display = "block";
                    document.getElementById("foodLabel" + index).style.display = "block";
                    document.getElementById("exercise" + index).style.display = "none";
                    document.getElementById("exerciseLabel" + index).style.display = "none";
                    document.getElementById("newFoodTitle" + index).style.display = "none";
                    document.getElementById("newFoodNameLabel" + index).style.display = "none";
                    document.getElementById("newFoodNameInput" + index).style.display = "none";
