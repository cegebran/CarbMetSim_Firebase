                deletedIDClone.id = "deletedID" + i
                deletedIDClone.value = "";
                p.parentNode.appendChild(deletedIDClone);

                if(existing == true){
                    document.getElementById("activity" + i).value = activity_type;

                    document.getElementById("dayInLine" + i).childNodes[0].value = day;
                    document.getElementById("dayInLine" + i).childNodes[2].value = hour;
                    document.getElementById("dayInLine" + i).childNodes[4].value = minute;

                    hourArray[i - 1] = hour;
                    minuteArray[i - 1] = minute;
                    dayArray[i - 1] = day;

                    if(activity_type == "Food"){
                        document.getElementById("food" + i).value = subtype;
                        document.getElementById("foodQtyInputID" + i).value = quantity;

                        document.getElementById("food" + i).style.display = "block";
                        document.getElementById("foodLabel" + i).style.display = "block";
                        document.getElementById("foodQtyLabel" + i).style.display = "block";
                        document.getElementById("foodQtyInputID" + i).style.display = "block";

