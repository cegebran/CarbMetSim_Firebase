            function activityTypeChange(id){
                var activities = document.getElementById(id);
                var index = id.slice(8);
                if(activities.value == "Exercise")
                {
                    food_exercise[index - 1] = "Exercise";
                    document.getElementById("food" + index).style.display = "none";
                    document.getElementById("foodLabel" + index).style.display = "none";
