            function hourChange(value, element){
                var parentId = element.parentNode.id;
                var number = parseInt(parentId.substring(9));
                hourArray[number - 1] = value;
            }

            function minuteChange(value, element){
                var parentId = element.parentNode.id;
                var number = parseInt(parentId.substring(9));
                minuteArray[number - 1] = value;
            }

            function dayChange(value, element){
                var parentId = element.parentNode.id;
                var number = parseInt(parentId.substring(9));
                dayArray[number - 1] = value;
            }

            function calculateTimeValue(day, hour, minute){
                var dayValue = (day - 1) * 1440;
                var hourValue = hour * 60;
                return (dayValue + hourValue + minute);
            }

            function validateForm(){
                var i = 0;
                var issues = [];

                if(deletedF_notDeletedT[0] == null){
                    issues.push("- At least 1 activity must be entered to run the simulation");
                }else{
                    var ctTrue = 0;
                    var k = 0;
                    while(deletedF_notDeletedT[k] != null){
                        if(deletedF_notDeletedT[k] == true){
                            ctTrue++;
                        }
                        k++;
                    }

                    if(ctTrue == 0){
                        issues.push("- At least 1 activity must be entered to run the simulation");
                    }
                }

                var endTimeCompleted = true;

                var endTimeGood = true;

                if(document.getElementById("endTimeDayInput").value == ""){
                    issues.push("- Simulation end day must be entered");
                    document.getElementById("endTimeDayInput").style.borderColor = "red";
                    endTimeCompleted = false;
                }else{
