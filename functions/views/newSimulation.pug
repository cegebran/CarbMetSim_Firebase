                newExerciseNameLabelClone.id = "newExerciseNameLabel" + i
                p.parentNode.appendChild(newExerciseNameLabelClone);

                var newExerciseNameInputOriginal = document.getElementById('newExerciseNameInput0');
                var newExerciseNameInputClone = newExerciseNameInputOriginal.cloneNode(true);
                newExerciseNameInputClone.id = "newExerciseNameInput" + i
                newExerciseNameInputClone.value = "";
                p.parentNode.appendChild(newExerciseNameInputClone);

                var newExerciseIntensityLabelOriginal = document.getElementById('newExerciseIntensityLabel0');
                var newExerciseIntensityLabelClone = newExerciseIntensityLabelOriginal.cloneNode(true);
                newExerciseIntensityLabelClone.id = "newExerciseIntensityLabel" + i
                p.parentNode.appendChild(newExerciseIntensityLabelClone);

                var newExerciseIntensityInputOriginal = document.getElementById('newExerciseIntensityInput0');
                var newExerciseIntensityInputClone = newExerciseIntensityInputOriginal.cloneNode(true);
                newExerciseIntensityInputClone.id = "newExerciseIntensityInput" + i
                newExerciseIntensityInputClone.value = "";
                p.parentNode.appendChild(newExerciseIntensityInputClone);
                
                var dayNumberLabelOriginal = document.getElementById("dayNumberLabel0");
                var dayNumberLabelClone = dayNumberLabelOriginal.cloneNode(true);
                dayNumberLabelClone.id = "dayNumberLabel" + i
                p.parentNode.appendChild(dayNumberLabelClone);

